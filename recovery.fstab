# mount point	    fstype	device	                		
/boot				emmc	/dev/block/mmcblk0p8	flags=backup=1
/recovery			emmc	/dev/block/mmcblk0p16	flags=backup=1
/cache				ext4	/dev/block/mmcblk0p15
/system				ext4	/dev/block/mmcblk0p12	flags=backup=1
/data				ext4	/dev/block/mmcblk0p13	length=-16384
/internal_sd		vfat	/dev/block/mmcblk0p20	flags=display="Internal Storage";storage;wipeingui
/sdcard				vfat	/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="External SD";storage;wipeingui;removable
